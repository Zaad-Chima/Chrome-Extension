{
    "manifest_version":2,
    "name":"TimeZone",
    "version":"2.5.1",
    "description":"This is a simple extension that displays the current time zone.",
    "icons":{
        "128":"clock__1__360.png"
    },
    "browser_action":{
        "default-popup":"popup.html",
        "default-icon":"clock.png",
        "default-title":"TimeZone"
    },
    "permissions":["activeTab"],
    "background":{
        "scripts":["main.js"]
    }
}